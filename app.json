{
  "expo": {
    "name": "TrackBing",
    "slug": "TrackBing",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/TrackBingLogo.png",
    "scheme": "trackbing",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kobinggg.TrackBing"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/TrackBingLogo.png",
      "name": "TrackBing",
      "shortName": "TrackBing",
      "display": "standalone",
      "backgroundColor": "#121212",
      "themeColor": "#FFD700",
      "description": "Track your calories and macros.",
      "orientation": "portrait"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ef27348-f0f2-4217-a0d3-b86aa4bdb498"
      }
    }
  }
}
